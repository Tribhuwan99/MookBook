<body>
  <div class="container">
    <h1>All Quiz Questions</h1>

    <% questions.forEach(function(ques){%>
    <div class="question-card">
      <h3><%=ques.Question%>?</h3>
      <ul>
        <li>A. <%=ques.Opa%></li>
        <li>B. <%=ques.Opb%></li>
        <li>C. <%=ques.Opc%></li>
        <li>D. <%=ques.Opd%></li>
      </ul>
      <p><strong>Heading:</strong> <%=ques.Title%></p>
      <p><strong>Marks:</strong> <%=ques.Marks%></p>
      <!-- <p><strong>Explanation:</strong> Delhi is the capital city of India.</p> -->
    </div>
<%})%>
